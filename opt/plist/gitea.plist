<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>homebrew.mxcl.gitea.plist</string>
    <key>EnvironmentVariables</key>
    <dict>
      <key>HOME</key>
      <string>/Users/abdulhakeem</string>
    </dict>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/homebrew/bin/gitea</string>
      <string>web</string>
      <string>--config</string>
      <string>/Users/abdulhakeem/.config/gitea/app.ini</string>
    </array>
    <key>Username</key>
    <string>abdulhakeem</string>
    <key>RunAtLoad</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/opt/homebrew/bin</string>
    <key>StandardOutPath</key>
    <string>/var/log/gitea.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/gitea-error.log</string>
    <key>SoftResourceLimits</key>
    <dict>
      <key>NumberOfFiles</key>
      <integer>8192</integer>
    </dict>
  </dict>
</plist>
